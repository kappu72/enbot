# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_bot_token_here
ALLOWED_GROUP_ID=your_group_id_here

# Optional: Admin User IDs (comma-separated)
ADMIN_USER_IDS=123456789,987654321

# Supabase Configuration (auto-configured in Edge Functions)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_API_KEY=your_anon_key_here

// Only for local development
SUPABASE_ACCESS_TOKEN=sbp_r03lt034mt2t0i24ei....

# Google Sheets Integration (Optional)
# To enable Google Sheets integration, create a service account in Google Cloud Console
# with access to Google Sheets API, download the JSON key file, and base64 encode it
GOOGLE_SERVICE_ACCOUNT_KEY=base64_encoded_service_account_json
GOOGLE_SPREADSHEET_ID=your_spreadsheet_id_here
GOOGLE_SHEET_NAME=Transactions

// True the bot will try to configure commands in group menu
SETUP_COMMANDS_ON_START=true